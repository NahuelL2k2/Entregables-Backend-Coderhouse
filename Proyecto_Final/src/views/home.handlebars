<h1>Productos</h1>
<div>
    {{#each productList}}
    <div id={{this.id}}>
        <div>
            {{!-- <img src={{this.thumbnail}} alt=""> --}}
            <h2>{{this.title}}</h2>
        </div>
        <div>
            <p>{{this.category}}</p>
            <p>{{this.description}}</p>
            <p>{{this.price}}</p>
            <p>{{this.stock}}</p>
            <p>{{this.code}}</p>
        </div>
    </div>
    {{/each}}
</div>